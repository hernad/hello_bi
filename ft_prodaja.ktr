  <step>
    <name>get_fakt</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>F18_bringout</connection>
    <sql>select fmk.fakt_doks.idtipdok, fakt_doks.brdok, fakt_doks.datdok as dat, fakt_fakt.kolicina, fakt_fakt.cijena *  (1 - COALESCE(fakt_fakt.rabat,0)&#47;100) as cijena, fakt_fakt.rabat, fakt_fakt.kolicina * fakt_fakt.cijena * (1 - COALESCE(fakt_fakt.rabat,0)&#47;100) as vrijednost, rtrim(fakt_fakt.idroba) as idroba, rtrim(fakt_doks.idpartner) as idpartner
from  fmk.fakt_fakt, fmk.fakt_doks 
where 
fakt_fakt.idfirma = fakt_doks.idfirma and fakt_fakt.idtipdok = fakt_doks.idtipdok and fakt_fakt.brdok = fakt_doks.brdok
and fakt_fakt.idtipdok=&apos;10&apos; 
order by fakt_doks.idfirma, fakt_doks.idtipdok, fakt_doks.brdok</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
